{
    "name": "patieru/signed-url",
    "description": "Signed (unique) URL package for Laravel.",
    "keywords": [
        "laravel",
        "signed",
        "url",
        "unique",
        "unique link",
        "unique url"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":"Ruberandinda Patience",
            "email":"info@schooloak.com",
            "homepage":"https://acounting.schooloak.com",
            "role":"Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "patieru/url-signer": "0.0.4",
        "illuminate/support": ">=5.2",
        "illuminate/http": ">=5.2"
    },
    "autoload": {
        "psr-4": {
            "Patieru\\SignedUrl\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Patieru\\SignedUrl\\Provider"
            ],
            "aliases": {
                "SignedUrl": "Patieru\\SignedUrl\\Facade"
            }
        }
    }
}
